<% if flash[:alert] %>
  <div class="alert alert-warning mt-4" role="alert">
    <%= flash[:alert] %>
  </div>
<% end %>

<% if flash[:notice] %>
  <div class="alert alert-warning mt-4" role="alert">
    <%= flash[:notice] %>
  </div>
<% end %>


<% if flash[:warning] %>
  <div class="alert alert-warning mt-4" role="alert">
    <%= flash[:warning] %>
  </div>
<% end %>


<% if flash[:error] %>
  <div class="alert alert-warning mt-4" role="alert">
    <%= flash[:error] %>
  </div>
<% end %>
